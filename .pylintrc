[MASTER]
ignore=venv,migrations
jobs=1
load-plugins=

[MESSAGES CONTROL]
disable=C0114,C0115,C0116

[REPORTS]
output-format=colorized
reports=no

[FORMAT]
max-line-length=100
indent-string='    '

[DESIGN]
max-args=10
max-locals=15
max-returns=6
max-branches=12
max-statements=50

[LOGGING]
logging-modules=logging